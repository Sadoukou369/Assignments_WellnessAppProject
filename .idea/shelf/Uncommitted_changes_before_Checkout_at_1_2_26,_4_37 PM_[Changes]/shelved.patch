+from models.CalorieEntities import Meal, Workout
+
+
+class Day:
+    def __init__(self, date):
+        self.__date = date
+        self.__meals = []
+        self.__workouts = []
+
+    @property
+    def date(self):
+        return self.__date
+
+    def add_meal(self, meal: Meal):
+        self.__meals.append(meal)
+
+    def add_workout(self, workout: Workout):
+        self.__workouts.append(workout)
+
+    def meal_calories(self):
+        total = 0
+        for m in self.__meals:
+            total += m.calories
+        return total
+
+    def workout_calories(self):
+        total = 0
+        for w in self.__workouts:
+            total += w.burned
+        return total
+
+    def net_calories(self):
+        return self.meal_calories() - self.workout_calories()
+
+    def meals_to_string(self):
+        s = ""
+        for m in self.__meals:
+            s += "\t" + str(m) + "\n"
+        return s
+
+    def workouts_to_string(self):
+        s = ""
+        for w in self.__workouts:
+            s += "\t" + str(w) + "\n"
+        return s
+
+    def __str__(self):
+        return (
+            f"Date: {self.__date}\n"
+            f"Meals:\n{self.meals_to_string()}"
+            f"Workouts:\n{self.workouts_to_string()}\n"
+            f"Net Calories: {self.net_calories()}\n"
+        )
