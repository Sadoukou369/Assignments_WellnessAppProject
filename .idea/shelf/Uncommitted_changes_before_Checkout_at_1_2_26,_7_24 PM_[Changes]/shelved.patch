+        self.__meals = []
+        self.__workouts = []
+
+    @property
+    def date(self) -> date:
+        return self.__date
+
+    @date.setter
+    def date(self, value: date):
+        self.__date = value
+
+    @property
+    def meals(self) -> list:
+        return self.__meals
+
+    @meals.setter
+    def meals(self, value: list):
+        self.__meals = value
+
+    @property
+    def workouts(self) -> list:
+        return self.__workouts
+
